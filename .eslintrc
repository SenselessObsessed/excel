{
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true
	},
	"rules": {
		"semi": "off",
		"arrow-parens": "off",
		"comma-dangle": "off",
		"require-jsdoc": "off",
		"linebreak-style": "off",
		"object-curly-spacing": "off",
		"no-unused-vars": "off",
		"no-tabs": "off",
		"indent": "off",
		"no-trailing-spaces": "off",
		"max-len": "off",
		"operator-linebreak": "off",
		"prefer-const": "off",
		"space-before-function-paren": "off"
	},
	"plugins": ["jest"],
	"extends": ["eslint:recommended", "google"]
}
